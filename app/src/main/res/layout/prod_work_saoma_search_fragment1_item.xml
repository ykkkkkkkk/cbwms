<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:gravity="center"
    android:orientation="horizontal">


    <TextView
        android:id="@+id/tv_row"
        android:layout_width="32dp"
        android:layout_height="@dimen/_46dp"
        android:background="@drawable/back_style_gray_line_right"
        android:gravity="center"
        android:text="行号"
        android:textColor="@color/black"
        android:textSize="12sp" />

    <TextView
        android:id="@+id/tv_mtlPriceType"
        android:layout_width="@dimen/_78dp"
        android:layout_height="@dimen/_46dp"
        android:background="@drawable/back_style_gray_line_right"
        android:gravity="center"
        android:text="计价类型"
        android:textColor="@color/black"
        android:textSize="12sp" />

    <TextView
        android:id="@+id/tv_process"
        android:layout_width="@dimen/_78dp"
        android:layout_height="@dimen/_46dp"
        android:background="@drawable/back_style_gray_line_right"
        android:gravity="center"
        android:text="工序"
        android:textColor="@color/black"
        android:textSize="12sp" />

    <TextView
        android:id="@+id/tv_num"
        android:layout_width="108dp"
        android:layout_height="@dimen/_46dp"
        android:background="@drawable/back_style_gray_line_right"
        android:gravity="center"
        android:text="数量"
        android:textColor="@color/c_515151"
        android:textSize="12sp" />

    <TextView
        android:id="@+id/tv_price"
        android:layout_width="108dp"
        android:layout_height="@dimen/_46dp"
        android:background="@drawable/back_style_gray_line_right"
        android:gravity="center"
        android:text="单价"
        android:textColor="@color/c_515151"
        android:textSize="12sp" />

    <TextView
        android:id="@+id/tv_money"
        android:layout_width="@dimen/_68dp"
        android:layout_height="@dimen/_46dp"
        android:gravity="center"
        android:text="金额"
        android:textColor="@color/c_FF2200"
        android:textSize="14sp" />


</LinearLayout>
